// -321   ---> -123
import java.util.*;
class digit2
{
public static void main(String args[])
{
int n;
Scanner sc=new Scanner(System.in);
n=sc.nextInt();
 int r,s1=0,s=0;
        if(n<0)
        {
            s1=-1;
            n=n*-1;
         }
        else if(n>0)
            s1=1;
        while(n>0)
        {
            r= n%10;
            s=(s*10)+r;
            n=n/10;
        }
System.out.println(s*s1);
}
}
