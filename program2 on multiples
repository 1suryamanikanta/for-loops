//  print 1.........40----> number 2,5 ----> mother
     // number 3,5  ---> father
// otherwise just print number
class sample1
{
public static void main(String args[])
{
int i=1;
for(i=1;i<=40;i++)  // 1  2   3  ....40
{  
     if ( (i%2 == 0  ) && (i%5==0) )  // 10   20   30  40
               System.out.println("Mother");
    else if ((i%3==0) &&(i%5==0))   //
        System.out.println("Father");
     else
            System.out.println(i);    
}

}
}
